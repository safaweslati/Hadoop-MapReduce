  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Current File",
      "request": "launch",
      "mainClass": "${file}"
    },
    {
      "type": "java",
      "name": "Main",
      "request": "launch",
      "mainClass": "hadoop.mapreduce.Main",
      "projectName": "wordcount"
    },
    {
      "type": "java",
      "name": "WordCount",
      "request": "launch",
      "mainClass": "hadoop.mapreduce.tp1.WordCount",
      "projectName": "wordcount",
      "args": [
        "wordcount/src/main/resources/input/file.txt",
        "wordcount/src/main/resources/output"
      ]
    }
  ]
}
